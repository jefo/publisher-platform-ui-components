{
  "name": "svite-example-routify-mdsvex-typescript",
  "version": "0.0.0",
  "scripts": {
    "validate": "svelte-check && tsc --noEmit",
    "dev": "run-p dev:routify dev:svite",
    "dev:routify": "routify run",
    "dev:svite": "svite -ts",
    "build": "run-s build:routify build:svite",
    "build:routify": "routify run -b",
    "build:svite": "svite build -ts"
  },
  "dependencies": {
    "@roxi/routify": "^2.7.3"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^1.0.10",
    "mdsvex": "^0.8.8",
    "npm-run-all": "^4.1.5",
    "svelte-check": "^1.1.15",
    "svelte-preprocess": "^4.6.1",
    "svelte": "^3.29.7",
    "svelte-hmr": "^0.11.6",
    "svite": "latest",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  }
}
